{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "printWidth": 80,
  "arrowParens": "avoid",
  "trailingComma": "all",
  "bracketSpacing": false,
  "bracketSameLine": true,
  "overrides": [
    {
      "files": [".ts", ".tsx"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
